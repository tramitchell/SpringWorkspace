package bankaccountapp;

public class BankAccountApp {
	
	public static void main(String[] args) {
		
		Checking chkAcc1 = new Checking("Tom Wilson", "123456789", 1500);
		Savings savAcc1 = new Savings("Rich Lowe", "152643879", 2500);

		savAcc1.showInfo();
		System.out.println("+++++++++++++++++++++++");
		chkAcc1.showInfo();
		
		
		
		savAcc1.compound();
		chkAcc1.compound();
		
	}
		
	
	
}
