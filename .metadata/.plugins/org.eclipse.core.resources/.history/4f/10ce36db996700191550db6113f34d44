package com.revature.interfaces;

public class Dog extends Wolf implements Domesticated{

	private String owner;
	
	public Dog() {
		this.owner = "Tucker";
	}
	@Override
	public String getOwner() {
		return this.owner;
	}

	@Override
	public void speak() {
		System.out.println("Bark! Bark!");
	}

}
