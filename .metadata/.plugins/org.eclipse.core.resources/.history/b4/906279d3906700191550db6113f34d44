package com.revature.varargs;

public class FunWithVarargs {
	//Var args- variable length arguments
	//a method that will take a variable number of arguments
	public static void main(String[] args) {
		
	}
	
	public static void vaTest(int ... v) {// the ... is how we declare  a var args
		System.out.print("Number of arguments is: " + v.length + " Contents: ");
		for(int x: v)
			System.out.println(x);
	}
}
